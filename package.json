{
  "name": "nuxt-typed-router",
  "version": "0.3.0",
  "description": "Provide autocompletion for pages route names generated by Nuxt router",
  "main": "lib/module.js",
  "keywords": [
    "nuxt typed router",
    "nuxt router",
    "nuxt typed",
    "nuxt safe router",
    "nuxt typed routes",
    "nuxt generate route ts"
  ],
  "scripts": {
    "dev": "tsc -p ./tsconfig.json --pretty --watch",
    "build": "rimraf lib && rimraf types && tsc -p ./tsconfig.json --pretty",
    "copy-files": "copyfiles -u 1 ./src/**/*.js ./lib",
    "prepack": "yarn build",
    "test": "jest",
    "test:reset": "yarn test --updateSnapshot"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "lib",
    "types/index.d.ts",
    "types/types.d.ts",
    "types/vue.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://victorgarciaesgi@github.com/victorgarciaesgi/nuxt-typed-router.git"
  },
  "author": {
    "name": "Victor Garcia",
    "url": "https://github.com/victorgarciaesgi"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/victorgarciaesgi/nuxt-typed-router/issues"
  },
  "typings": "types/index.d.ts",
  "dependencies": {
    "chalk": "^4.1.2",
    "jest": "^27.2.4",
    "lodash": "^4.17.21",
    "log-symbols": "^5.0.0",
    "prettier": "^2.4.1"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@nuxt/test-utils": "^0.2.2",
    "@nuxt/types": "^2.15.8",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.173",
    "@types/node": "^15.12.4",
    "@types/prettier": "^2.3.2",
    "babel-jest": "^27.2.4",
    "copyfiles": "^2.4.0",
    "core-js": "^3.18.2",
    "nuxt": "^2.15.8",
    "playwright": "^1.15.2",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.5",
    "typescript": "^4.4.3",
    "vue-router": "^3.5.2"
  }
}
